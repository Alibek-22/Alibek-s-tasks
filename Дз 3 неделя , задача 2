def простые_множители(x):
    i = 2
    множители = []
    while i * i <= x:
        if x % i:
            i += 1
        else:
            x //= i
            множители = множители + [i]
    if x > 1:
        множители = множители + [x]
    return множители

число = int(input("Введите число: "))
print("Простые множители:", простые_множители(число))
